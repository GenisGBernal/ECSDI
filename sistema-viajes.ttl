@prefix : <http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes/> .

<http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes> rdf:type owl:Ontology ;
                                                                       rdfs:comment "Ontología para sistema de Viajes de la asignatura ECSDI" .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#a_planificar
:a_planificar rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :PedirActividades ,
                          :PedirHospedaje ,
                          :PedirTransporte ,
                          :PlaneaViaje ,
                          :RespuestaActividades ,
                          :RespuestaHospedajes ,
                          :RespuestaTransportes ;
              rdfs:range :PeticionDeViaje .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#actividad_mañana
:actividad_mañana rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain :PlanDia ;
                  rdfs:range :Actividad .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#actividad_noche
:actividad_noche rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain :PlanDia ;
                 rdfs:range :Actividad .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#actividad_tarde
:actividad_tarde rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain :PlanDia ;
                 rdfs:range :Actividad .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#actividades_ordenadas
:actividades_ordenadas rdf:type owl:ObjectProperty ;
                       rdfs:domain :Viaje ;
                       rdfs:range :VisitaTuristicoCultural ;
                       rdfs:comment "Actividades ordenadas. Tal y como se expresa en el enunciado, una visita turisstico-cultural consiste en una lista de actividades por dia ordenadas y dentro de cada PlanDia actividades para mañana tarde y noche" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#cliente
:cliente rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :CobrarViaje ,
                     :CobroRechazado ,
                     :Pago ,
                     :PlaneaViaje ,
                     :Recomendacion ;
         rdfs:range :Usuario .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#prevision
:prevision rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :PedirMeteorologia ,
                       :RespuestaMeteorologia ;
           rdfs:range :PrediccionMeteorologica .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#primer_dia
:primer_dia rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :VisitaTuristicoCultural ;
            rdfs:range :PlanDia .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#siguiente_dia
:siguiente_dia rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :PlanDia ;
               rdfs:range :PlanDia .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#tiene_viaje
:tiene_viaje rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :CobrarViaje ,
                         :CobroRechazado ,
                         :Pago ,
                         :PlaneaViaje ,
                         :ReemplazaActividades ,
                         :RemplazamientosActividades ;
             rdfs:range :Viaje .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#viaje_actividades
:viaje_actividades rdf:type owl:ObjectProperty ;
                   rdfs:domain :PedirActividades ,
                               :PedirActividadesCanceladas ,
                               :PedirActividadesSimilares ,
                               :PedirMeteorologia ,
                               :Recomendacion ,
                               :ReemplazaActividades ,
                               :RemplazamientosActividades ,
                               :RespuestaActividadesCanceladas ,
                               :RespuestaActividadesSimilares ,
                               :RespuestaMeteorologia ;
                   rdfs:range :Actividad .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#viaje_ciudad
:viaje_ciudad rdf:type owl:ObjectProperty ;
              rdfs:domain :PeticionDeViaje ,
                          :Recomendacion ,
                          :Viaje ;
              rdfs:range :Ciudad .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#viaje_finalizado
:viaje_finalizado rdf:type owl:ObjectProperty ;
                  rdfs:domain :Satisfaccion ;
                  rdfs:range :ViajeFinalizado .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#viaje_hospedaje
:viaje_hospedaje rdf:type owl:ObjectProperty ;
                 rdfs:domain :PedirHospedaje ,
                             :PeticionDeViaje ,
                             :Viaje ;
                 rdfs:range :Hospedaje .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#viaje_transporte
:viaje_transporte rdf:type owl:ObjectProperty ;
                  rdfs:domain :PedirTransporte ,
                              :PeticionDeViaje ,
                              :Viaje ;
                  rdfs:range :Transporte .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain :PlaneaViaje .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#centrico
:centrico rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :PeticionDeViaje ;
          rdfs:range xsd:boolean .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#dato_bancario
:dato_bancario rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :CobrarViaje ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#fecha_confirmacion
:fecha_confirmacion rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Pago ,
                                :ViajeConfirmado ;
                    rdfs:range xsd:dateTimeStamp ;
                    rdfs:comment "Fecha de confirmación de un acontecimiento" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#fecha_llegada
:fecha_llegada rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :PeticionDeViaje ,
                           :Viaje ;
               rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#fecha_salida
:fecha_salida rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :PeticionDeViaje ,
                          :Viaje ;
              rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#grado_cultural
:grado_cultural rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :PeticionDeViaje ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "Algo" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "Mucho" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "Nada" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "Normal" ;
                                                                          rdf:rest rdf:nil
                                                                        ]
                                                             ]
                                                  ]
                                       ]
                           ] .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#grado_festivo
:grado_festivo rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :PeticionDeViaje ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "Algo" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "Mucho" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "Nada" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "Normal" ;
                                                                         rdf:rest rdf:nil
                                                                       ]
                                                            ]
                                                 ]
                                      ]
                          ] .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#grado_ludica
:grado_ludica rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :PeticionDeViaje ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:oneOf [ rdf:type rdf:List ;
                                       rdf:first "Algo" ;
                                       rdf:rest [ rdf:type rdf:List ;
                                                  rdf:first "Mucho" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "Nada" ;
                                                             rdf:rest [ rdf:type rdf:List ;
                                                                        rdf:first "Normal" ;
                                                                        rdf:rest rdf:nil
                                                                      ]
                                                           ]
                                                ]
                                     ]
                         ] .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#hay_lluvia_matina
:hay_lluvia_matina rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain :PrediccionMeteorologica ;
                   rdfs:range xsd:boolean .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#hay_lluvia_noche
:hay_lluvia_noche rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :PrediccionMeteorologica ;
                  rdfs:range xsd:boolean .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#hay_lluvia_tarde
:hay_lluvia_tarde rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain :PrediccionMeteorologica ;
                  rdfs:range xsd:boolean .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#identificador
:identificador rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Actividad ,
                           :Hospedaje ,
                           :Lugar ,
                           :Pago ,
                           :PeticionDeViaje ,
                           :Transporte ,
                           :Usuario ,
                           :Viaje ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#motivo_rechazo
:motivo_rechazo rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :CobroRechazado ;
                rdfs:range xsd:string .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#precio
:precio rdf:type owl:DatatypeProperty ;
        rdfs:domain :Viaje ;
        rdfs:range owl:real ;
        rdfs:comment "Precio de un elemento (viaje, ...)" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#precio_maximo
:precio_maximo rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :PeticionDeViaje ;
               rdfs:range owl:real .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#precio_minimo
:precio_minimo rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :PeticionDeViaje ;
               rdfs:range owl:real .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#valoracion
:valoracion rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Satisfaccion ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first 1 ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first 2 ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first 3 ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first 4 ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first 5 ;
                                                                                 rdf:rest rdf:nil
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                   ]
                       ] .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:domain :Actividad ,
                                :Hospedaje ,
                                :Lugar ,
                                :PeticionDeViaje ,
                                :Transporte ,
                                :Usuario ,
                                :Viaje ;
                    rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Accion
:Accion rdf:type owl:Class ;
        rdfs:comment """formato: (Protocolo:Mensaje)
Mensajes que piden a un agente realizar una acción""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Actividad
:Actividad rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           rdfs:comment "Actividades a realizar durante una estancia" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Ciudad
:Ciudad rdf:type owl:Class ;
        rdfs:subClassOf :Lugar .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#CobrarViaje
:CobrarViaje rdf:type owl:Class ;
             rdfs:subClassOf :Accion ;
             rdfs:comment """\"Comunica cobro\".\"QuieroCobrarViaje\"
Mensaje que contiene la informacion del cobro del viaje""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#CobroAcceptado
:CobroAcceptado rdf:type owl:Class ;
                rdfs:subClassOf :Informacion ;
                rdfs:comment """\"Comunica cobro\".\"TomaCobroAceptado\"
Mensaje que indica que el cobro ha sido acceptado correctamente""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#CobroRechazado
:CobroRechazado rdf:type owl:Class ;
                rdfs:subClassOf :Informacion ;
                rdfs:comment """\"Comunica cobro\".\"TomaCobroRechazado\"
Mensaje que indica que el cobro se ha rechazado""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Hospedaje
:Hospedaje rdf:type owl:Class ;
           rdfs:subClassOf owl:Thing ;
           rdfs:comment "Hospedaje donde residir durante un viaje" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Informacion
:Informacion rdf:type owl:Class ;
             rdfs:comment """formato: (Protocolo:Mensaje)
Mensaje informativo de un agente a otro""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Lugar
:Lugar rdf:type owl:Class ;
       rdfs:subClassOf owl:Thing ;
       rdfs:comment "Representa los distintos tipos de lugares (ciudades, puntos de interes, ...)" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Pago
:Pago rdf:type owl:Class ;
      rdfs:comment "elemento que representa el pago de un viaje por un usuario" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirActividades
:PedirActividades rdf:type owl:Class ;
                  rdfs:subClassOf :Accion ;
                  rdfs:comment """\"Comunica actividades\":\"QuieroActividades\"
Envia un PeticionDeViaje de donde se extrae la informacion sobre las fechas y rangos de precio""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirActividadesCanceladas
:PedirActividadesCanceladas rdf:type owl:Class ;
                            rdfs:subClassOf :Accion ;
                            rdfs:comment "\"Comunica actividades canceladas\":\"QuieroActividadesCanceladas\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirActividadesSimilares
:PedirActividadesSimilares rdf:type owl:Class ;
                           rdfs:subClassOf :Accion ;
                           rdfs:comment """\"Comunica actividades similares\":\"QuieroActividadesSimilares\"
Mensaje que envia el Agente de cancelaciones al planificador con un viaje concreto y sus actividades a reemplazar. Se envia por cada viaje que haya""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirHospedaje
:PedirHospedaje rdf:type owl:Class ;
                rdfs:subClassOf :Accion ;
                rdfs:comment """\"Comunica hospedaje\":\"QuieroHospedaje\"
Agente solicitando hospedaje para unas condiciones concretas
Envia un PeticionDeViaje de donde se extrae la informacion sobre las fechas y rangos de precio""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirMeteorologia
:PedirMeteorologia rdf:type owl:Class ;
                   rdfs:subClassOf :Accion ;
                   rdfs:comment "\"Comunica prevision meteorologica\":\"QuieroPrevisionMeteorologica\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PedirTransporte
:PedirTransporte rdf:type owl:Class ;
                 rdfs:subClassOf :Accion ;
                 rdfs:comment """\"Comunica transporte\":\"QuieroTransporte\"
Envia un PeticionDeViaje de donde se extrae la informacion sobre las fechas y rangos de precio""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PeticionDeViaje
:PeticionDeViaje rdf:type owl:Class ;
                 rdfs:subClassOf owl:Thing ;
                 rdfs:comment "Clase que representa la Informacion contenida en una petición de Viaje" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PlanDia
:PlanDia rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ;
         rdfs:comment "Consiste en la planificacion de actividades para un dia del viaje (actividad de mañana, tarde y noche)" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PlaneaViaje
:PlaneaViaje rdf:type owl:Class ;
             rdfs:subClassOf :Accion ;
             rdfs:comment """\"Comunica plan de viaje\":\"QuieroPlanViaje\"
Pedirle al planificador que planee un viaje con la información proporcionada""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PrediccionMeteorologica
:PrediccionMeteorologica rdf:type owl:Class ;
                         rdfs:comment "Predicción meteorologica del próximo día" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#PropuestaViaje
:PropuestaViaje rdf:type owl:Class ;
                rdfs:subClassOf :Informacion ;
                rdfs:comment """\"Comunica plan de viaje\":\"TomaPlanViaje\"
Viaje entero planificado""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Recomendacion
:Recomendacion rdf:type owl:Class .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#ReemplazaActividades
:ReemplazaActividades rdf:type owl:Class ;
                      rdfs:subClassOf :Accion ;
                      rdfs:comment """\"Comunica actividades a reemplazar\":\"QuieroActividadesReemplazadas\"
Mensaje que envia el Agente de cancelaciones al planificador con un viaje concreto y sus actividades a reemplazar. Se envia por cada viaje que haya""" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RemplazamientosActividades
:RemplazamientosActividades rdf:type owl:Class ;
                            rdfs:subClassOf :Informacion ;
                            rdfs:comment "\"Comunica actividades a reemplazar\":\"TomaActividadesReemplazadas\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaActividades
:RespuestaActividades rdf:type owl:Class ;
                      rdfs:subClassOf :Informacion ;
                      rdfs:comment "\"Comunica actividades\":\"TomaActividades\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaActividadesCanceladas
:RespuestaActividadesCanceladas rdf:type owl:Class ;
                                rdfs:subClassOf :Informacion ;
                                rdfs:comment "\"Comunica actividades canceladas\":\"TomaActividadesCanceladas\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaActividadesSimilares
:RespuestaActividadesSimilares rdf:type owl:Class ;
                               rdfs:subClassOf :Informacion ;
                               rdfs:comment "\"Comunica actividades similares\":\"TomaActividadesSimilares\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaHospedajes
:RespuestaHospedajes rdf:type owl:Class ;
                     rdfs:subClassOf :Informacion ;
                     rdfs:comment "\"Comunica hospedaje\":\"TomaHospedaje\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaMeteorologia
:RespuestaMeteorologia rdf:type owl:Class ;
                       rdfs:subClassOf :Informacion ;
                       rdfs:comment "\"Comunica prevision meteorologica\":\"TomaPrevisionMeteorologica\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#RespuestaTransportes
:RespuestaTransportes rdf:type owl:Class ;
                      rdfs:subClassOf :Informacion ;
                      rdfs:comment "\"Comunica Transporte\":\"TomaTransporte\"" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Satisfaccion
:Satisfaccion rdf:type owl:Class ;
              rdfs:comment "Registra la satisfaccion de un viaje" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Transporte
:Transporte rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            rdfs:comment "Transporte requerido para el viaje" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Usuario
:Usuario rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing ;
         rdfs:comment "Representa un cliente, su perfil" .


###  http://www.semanticweb.org/buffalo/ontologies/2023/4/sistema-viajes#Viaje
:Viaje rdf:type owl:Class ;
